void main() {
  // o_FragColor = vec4(v_Uv, 0., 1.);
  o_FragColor = texture(u_Sprite, v_Uv);
  o_FragColor.a = o_FragColor.a * (1. - pow(v_Age/v_Life, 6.));
}
