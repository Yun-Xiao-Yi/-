*e}function Fr(e,t,n){return 3*Sr(t,n)*e*e+2*$r(t,n)*e+Dr(t)}function m4(e,t,n,i,o){let l,s,r=0;do s=t+(n-t)/2,l=li(s,i,o)-e,l>0?n=s:t=s;while(Math.abs(l)>u4&&++r<h4);return s}function f4(e,t,n,i){for(let o=0;o<c4;++o){const l=Fr(t,n,i);if(l===0)return t;t-=(li(t,n,i)-e)/l}return t}function v4(e){return e}function _4(e,t,n,i){if(!(0<=e&&e<=1&&0<=n&&n<=1))throw new Error("bezier x values must be in [0, 1] range");if(e===t&&n===i)return v4;const o=p4?new Float32Array(_n):new Array(_n);for(let s=0;s<_n;++s)o[s]=li(s*Un,e,n);function l(s){let r=0,d=1;const h=_n-1;for(;d!==h&&o[d]<=s;++d)r+=Un;--d;const _=(s-o[d])/(o[d+1]-o[d]),f=r+_*Un,p=Fr(f,e,n);return p>=d4?f4(s,f,e,n):p===0?f:m4(s,r,r+Un,e,n)}return function(r){return r===0||r===1?r:li(l(r),t,i)}}const Xa=(e,t,n)=>{const i=e.createShader(t);if(!i)throw new Error("can not create shader");if(e.shaderSource(i,n),e.compileShader(i),!e.getShaderParameter(i,e.COMPILE_STATUS)){const o=`An error occurred compiling the shaders: ${e.getShaderInfoLog(i)}`;throw e.deleteShader(i),new Error(o)}return i};class qa{constructor({gl:t,vs:n,fs:i,transformFeedbackVaryings:o}){ce(this,"gl");ce(this,"program");ce(this,"locations");ce(this,"uniformBuffers");const l=Xa(t,t.VERTEX_SHADER,n),s=Xa(t,t.FRAGMENT_SHADER,i),r=t.createProgram();if(!r)throw new Error("can not create shader program");if(t.attachShader(r,l),t.attachShader(r,s),o&&t.transformFeedbackVaryings(r,o,t.INTERLEAVED_ATTRIBS),t.linkProgram(r),t.deleteShader(l),t.deleteShader(s),!t.getProgramParameter(r,t.LINK_STATUS))throw new Error(`Unable to initialize the shader program: ${t.getProgramInfoLog(r)}`);this.gl=t,this.program=r,this.locations=new Map,this.uniformBuffers={}}use(){this.gl.useProgram(this.program)}setUniform(t,n,i){let o=this.locations.get(t);switch(o||(o=this.gl.getUniformLocation(this.program,t),this.locations.set(t,o)),n){case"BOOLEAN":return this.gl.uniform1i(o,Number(i));case"INT":return this.gl.uniform1i(o,Math.round(i));case"FLOAT":return this.gl.uniform1f(